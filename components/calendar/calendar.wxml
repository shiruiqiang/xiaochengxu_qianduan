<!--   calendar    -->
<template name="calendar"><block wx:if="undefined"><view bindtap="{{hide}}" class="weui-mask {{animateCss[1]}}" hidden="{{!visible}}"></view></block><block wx:if="undefined"><view class="picker-modal picker-calendar {{inline?'picker-modal-inline':''}} {{direction==='vertical'?'picker-calendar-vertical':'picker-calendar-horizontal'}} {{animateCss[0]}}"><block wx:if="undefined"><view class="toolbar"><view class="toolbar-inner"><view class="picker-calendar-month-picker"><view bindtap="{{prevMonth}}" class="link icon-only picker-calendar-prev-month"><view class="icon icon-prev"></view></view><view class="current-month-value">\n						{{currentMonthName}}\n</view><view bindtap="{{nextMonth}}" class="link icon-only picker-calendar-next-month"><view class="icon icon-next"></view></view></view><view class="picker-calendar-year-picker"><view bindtap="{{prevYear}}" class="link icon-only picker-calendar-prev-year"><view class="icon icon-prev"></view></view><text class="text current-year-value">{{currentYear}}</text><view bindtap="{{nextYear}}" class="link icon-only picker-calendar-next-year"><view class="icon icon-next"></view></view></view></view></view></block><view class="picker-modal-inner"><block wx:if="undefined"><view class="picker-calendar-week-days"><block wx:for="{{weeks}}" wx:for-item="item"><view class="picker-calendar-week-day {{item.weekend?'picker-calendar-week-day-weekend':''}}">\n						{{item.dayName}}\n</view></block></view></block><view class="picker-calendar-months"><view bindtouchend="{{handleTouchEnd}}" bindtouchmove="{{handleTouchMove}}" bindtouchstart="{{handleTouchStart}}" class="picker-calendar-months-wrapper" style="{{style}}"><block wx:for="{{months}}" wx:for-item="month"><view class="picker-calendar-month {{index===0?'picker-calendar-month-prev':index===1?'picker-calendar-month-current':'picker-calendar-month-next'}}" data-month="{{month.month}}" data-year="{{month.year}}"><block wx:for="{{month.items}}" wx:for-item="row"><view class="picker-calendar-row"><block wx:for="{{row}}" wx:for-item="col"><view bindtap="{{handleDayClick}}" class="picker-calendar-day {{col.type.prev?'picker-calendar-day-prev':''}} {{col.type.next?'picker-calendar-day-next':''}} {{col.type.today?'picker-calendar-day-today':''}} {{col.type.selected?'picker-calendar-day-selected':''}} {{col.type.weekend?'picker-calendar-day-weekend':''}} {{col.type.disabled?'picker-calendar-day-disabled':''}}" data-date="{{col.date}}" data-day="{{col.day}}" data-month="{{col.month}}" data-type="{{col.type}}" data-year="{{col.year}}"><text class="text">{{col.day}}</text></view></block></view></block></view></block></view></view></view></view></block></template>
