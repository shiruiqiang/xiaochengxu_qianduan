<!--   dialog    -->
<template name="dialog"><block wx:if="undefined"><view class="weui-mask {{animateCss}}"></view></block><block wx:if="undefined"><view class="weui-dialog {{animateCss}}"><block wx:if="undefined"><view class="weui-dialog__hd"><view class="weui-dialog__title">{{title}}</view></view></block><block wx:if="undefined"><view class="weui-dialog__bd"><view class="weui-dialog__content">{{content}}</view><block wx:if="undefined"><input bindinput="{{bindinput}}" class="weui-input weui-prompt-input" maxlength="{{maxlength}}" password="{{prompt.password}}" placeholder="{{prompt.placeholder}}" type="{{prompt.fieldtype}}" value="{{prompt.response}}"></input></block><block wx:if="undefined"><picker bindchange="{{bindPickerChange}}" range="{{selector.selectorData}}" rangeKey="'name'" value="{{selector.selectorData[selector.index].id}}"><view class="weui-input weui-selector-selectbox">{{selector.selectorData[selector.index].name?selector.selectorData[selector.index].name:'请选择职位'}}</view></picker></block></view></block><block wx:if="undefined"><view class="weui-dialog__ft {{verticalButtons?'weui-dialog__btns_vertical':''}}"><block wx:for="{{buttons}}" wx:for-item="item"><view bindtap="{{buttonTapped}}" class="weui-dialog__btn weui-dialog__btn_default {{item.type}} {{item.bold?'weui-dialog__btn_bold':''}}" data-index="{{index}}">{{item.text}}</view></block></view></block></view></block></template>
