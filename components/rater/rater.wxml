<!--   rater    -->
<template name="rater"><block wx:if="undefined"><view class="wux-rater {{animateCss}}"><block wx:for="{{stars}}" wx:for-item="item"><view bindtap="{{handlerClick}}" class="wux-rater-box {{value>index?'is-active':''}}" data-index="{{index}}" style="color: {{colors&&colors.index?colors.index:'#ccc'}}; margin-right: {{margin+'px'}}; font-size: {{fontSize+'px'}}; width: {{fontSize+'px'}}; height: {{fontSize+'px'}}; line-height: {{fontSize+'px'}}"><view class="wux-rater-inner">\n					{{star}}\n<block wx:if="undefined"><view class="wux-rater-outer" style="color: {{activeColor}}; width: {{cutPercent+'%'}}">\n						{{star}}\n</view></block></view></view></block><block wx:for="{{text}}" wx:for-item="item"><block wx:if="undefined"><view class="wux-rater-text {{item.className}}" style="color: {{item.color||defaultTextColor}}">\n				{{item.text||item}}\n</view></block></block></view></block></template>
